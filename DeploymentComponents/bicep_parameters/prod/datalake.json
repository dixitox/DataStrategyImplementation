{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "storageAccountType": {
      "value": "Standard_GRS"
    },
    "accessTier": {
      "value": "Hot"
    },
    "isHnsEnabled": {
      "value": true
    },
    "requireInfrastructureEncryption": {
      "value": true
    },
    "softDeleteEnabled": {
      "value": true
    },
    "DataLakeContainerNames": {
      "value": [
        "raw",
        "structured",
        "staging",
        "curated"
      ]
    },
    "deployblobPE": {
      "value": true
    },
    "deploydfsPE": {
      "value": true
    },
    "deployfilePE": {
      "value": false
    },
    "deployqueuePE": {
      "value": false
    },
    "deploytablePE": {
      "value": false
    },
    "enableDiagnostics": {
      "value": true
    }
  }
}