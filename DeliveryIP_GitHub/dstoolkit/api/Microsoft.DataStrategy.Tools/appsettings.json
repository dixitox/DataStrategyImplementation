{
  "Cosmos": {
    "Endpoint": "https://cosmos-dstoolkit-we-dev.documents.azure.com:443/",
    "Database": "DataStrategyDb",
    "AssetsContainer": "Assets"
  },
  "Search": {
    "Endpoint": "https://src-dstoolkit-we-dev.search.windows.net/",
    "AssetsIndex": "cosmosdb-index",
    "AssetsIndexer": "cosmosdb-indexer"
  },
  "Storage": {
    "AccountName": "stdstoolkitwedev",
    "ContainerName": "images",
    "ThumbnailsDirectory": "thumbnails",
    "ScreenshotsDirectory": "screenshots"
  },
  "ServiceBusConnection": {
    "FullyQualifiedNamespace": "sb-dstoolkit-we-dev.servicebus.windows.net",
    "AssetChangeNotificationQueue": "assetchangenotificationqueue"
  },
  "ImporterDirectory": "YamlToProcess/dstoolkit"
}

